! cleanbaja's X resources

! ------------------------------
!   Rxvt Unicode Configuration
! ------------------------------

URxvt.font: xft:Liberation:size=11
! Support opening URLs
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
! Remove scrollbar and add transparent background
URxvt.scrollBar: false
URxvt.depth: 32
URxvt.background: [95]#2e3440

